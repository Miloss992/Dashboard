<%- include('../partials/header') %>

    <main id="main" class="main">

        <div class="pagetitle">
            <h1>Delete Form</h1>
            <nav>
                <ol class="breadcrumb">
                    <li class="breadcrumb-item">Home</li>
                    <li class="breadcrumb-item active">Table View</li>
                </ol>
            </nav>
        </div>

        <section class="section contact">

            <div class="row gy-4">
                <div class="col-xl-12">
                    <h3>Are you sure you want to delete this?</h3>
                    <div class="card p-4">
                        <div class="row gy-4">

                            <dl class="row">
                                <dt class="col-sm-1">
                                    Segment
                                </dt>
                                <dd class="col-sm-11">
                                    <%= fData.segment %>
                                </dd>
                                <dt class="col-sm-1">
                                    Country
                                </dt>
                                <dd class="col-sm-11">
                                    <%= fData.country %>
                                </dd>
                                <dt class="col-sm-1">
                                    Product
                                </dt>
                                <dd class="col-sm-11">
                                    <%= fData.product %>
                                </dd>
                                <dt class="col-sm-1">
                                    DiscountBand
                                </dt>
                                <dd class="col-sm-11">
                                    <%= fData.discountBand %>
                                </dd>
                                <dt class="col-sm-1">
                                    UnitsSold
                                </dt>
                                <dd class="col-sm-11">
                                    <%= fData.unitsSold %>
                                </dd>
                                <dt class="col-sm-1">
                                    ManufacturingPrice
                                </dt>
                                <dd class="col-sm-11">
                                    <%= fData.manufacturingPrice %>
                                </dd>
                                <dt class="col-sm-1">
                                    SalePrice
                                </dt>
                                <dd class="col-sm-11">
                                    <%= fData.salePrice %>
                                </dd>
                                <dt class="col-sm-1">
                                    GrossSales
                                </dt>
                                <dd class="col-sm-11">
                                    <%= fData.grossSales %>
                                </dd>
                                <dt class="col-sm-1">
                                    Discounts
                                </dt>
                                <dd class="col-sm-11">
                                    <%= fData.discounts %>
                                </dd>
                                <dt class="col-sm-1">
                                    Sales
                                </dt>
                                <dd class="col-sm-11">
                                    <%= fData.sales %>
                                </dd>
                                <dt class="col-sm-1">
                                    CostsOfSales
                                </dt>
                                <dd class="col-sm-11">
                                    <%= fData.costsOfSales %>
                                </dd>
                                <dt class="col-sm-1">
                                    Profit
                                </dt>
                                <dd class="col-sm-11">
                                    <%= fData.profit %>
                                </dd>
                                <dt class="col-sm-1">
                                    Date
                                </dt>
                                <dd class="col-sm-11">
                                    <%= moment(fData.date).format( 'MM/DD/YYYY' ) %>
                                </dd>
                            </dl>

                            <div class="offset-md-1 text-left">
                                <form action="/Delete!" method="post">
                                    <button class="btn btn-danger mx-1" type="submit" name="delete"
                                        value="<%=fData._id %>">Delete</button>
                                </form>
                                <a href="Table" type="submit" class="btn btn-primary">Back to List</a>
                            </div>

                        </div>
                    </div>

                </div>

            </div>

        </section>

    </main>

    <%- include('../partials/footer') %>